{
    "version": 2,
    "builds": [{ "src": "app.js", "use": "@now/node-server" }],
    "routes": [
        {
            "src": "/",
            "dest": "app.js",
            "methods": ["GET"]
        },
        {
            "src": "/api",
            "dest": "app.js",
            "methods": ["GET"]
        },
        {
            "src": "/api/schedule",
            "dest": "app.js",
            "methods": ["POST"]
        }
    ]
}